);\n					View._doChoose((View._index + 1) % View._Creative.length);\n				});\n			};\n			LargeVideo.prototype._videoHideShow = function(_video, _videodiv, isShow) {\n				if (isShow) {\n					_video && (_video.style.display = \"block\");\n					_videodiv && (_videodiv.style.display = \"block\");\n				} else {\n					_videodiv && (_videodiv.style.display = \"none\");\n					_video && (_video.style.display = \"none\");\n				}\n			};\n\n			LargeVideo.prototype.reset = function() {\n				this._video = document.getElementById('video_' + View._index);\n				that._isCloseWindow = false;\n				this.play();\n			}\n			LargeVideo.prototype.play = function() {\n				this._video && this._video.play();\n			}\n			LargeVideo.prototype.stop = function() {\n				this._video && this._video.pause();\n			}\n			LargeVideo.prototype.cleanLastVideo = function() {\n				if (this._lastvideo) {\n					this._lastvideo.pause();\n					this._lastvideo.currentTime = 0.1;\n					this._videoHideShow(this._lastvideo, this._largeVideoDiv, false);\n				}\n				this._lastvideo = null;\n			}\n			LargeVideo.prototype._initialized = true;\n		}\n\n		VideoView.prototype.initVideoView = function(videoCreative) {\n			this._Creative = videoCreative;\n			this._ViewDiv = document.createElement('div');\n			this._ViewDiv.style.cssText = 'display:none;position:absolute;width:810px;height:360px;top:120px;left:105px;z-index:2';\n			this._ViewDiv.id = 'videoViewDiv';\n			document.getElementById('bgdiv').appendChild(this._ViewDiv);\n			this._init();\n		}\n\n		VideoView.prototype._doChoose = function(index) {\n			this._index = index;\n			this._changeVideo();\n		}\n\n		VideoView.prototype._init = function() {\n			this._largeVideo = new LargeVideo;\n			this._largeVideo.initLargeVideo(this);\n\n			this._choose = new Choose();\n			this._choose.initChoose(this);\n		};\n		VideoView.prototype.pause = function() {\n			this._largeVideo.stop();\n		};\n		VideoView.prototype.start = function() {\n			this._largeVideo.play();\n		}\n		VideoView.proto