hstart', function() {\n				that._executeSubscribe('ad_video_click', [that._assets.ads[0].vdjump, 0, true]);\n			});\n		};\n		Video.prototype.start = function() {\n			this._video.play();\n		};\n		Video.prototype.pause = function() {\n			this._video.pause();\n		};\n		Video.prototype._initialized = true;\n	};\n\n	//下方的互动栏\n	var ControlBar = function() {\n		this._controldiv = null;\n\n		this._sharebtn = null;\n		this._tdCodebtn = null;\n		this._logobtn = null;\n		this._videoSelbtn = null;\n		this._picSelbtn = null;\n		this._websitebtn = null;\n		this._selectedbtn = -1;\n\n		this._number = 0;\n		this._margin = 0;\n\n		if (typeof ControlBar.prototype._initialized != 'undefined') return;\n		ControlBar.prototype.initControlBar = function() {\n			this._controldiv = document.createElement('div');\n			this._controldiv.style.cssText = 'position:absolute;width:490px;height:70px;bottom:0px;left:50%;margin-left:-245px';\n			var innerhtml = '<img src=\"http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225180123_BOX.png\" style =\"width:100%;height:100%;position:absolute;z-index:-1\"/>';\n			this._controldiv.innerHTML = innerhtml;\n			this._controldiv.id = 'controlbar';\n\n			document.body.appendChild(this._controldiv);\n\n			this._number = this._getNumberOfBtn();\n			this._margin = (400 - 50 * this._number) / (2 + this._number);\n\n			this.createBtns();\n		}\n\n		ControlBar.prototype.createBtns = function() {\n			for (var i = 0, len = that._assets.ads.length; i < len; i++) {\n				switch (that._assets.ads[i].id) {\n					case that._btntype.logo:\n						if (this._logobtn) break;\n						this._logobtn = new Logo();\n						this._logobtn.initLogo(this._margin, i);\n						break;\n					case that._btntype.share:\n						if (this._sharebtn) break;\n						this._sharebtn = new Share();\n						this._sharebtn.initShare(this._margin, i);\n						break;\n					case that._btntype.tdCode:\n						if (this._tdCodebtn) break;\n						this._tdCodebtn = new TdCode();\n						this._tdCodebtn.initTdCode(this._margin, i);\n						break;\n					case that._btntype.videoSel:\n						if (this._videoSelbtn) break;\n						this._videoSelbtn = new VideoSel();\n						this._videoSelbtn.initVideoSel(this._margin, i);\n						break;\n					case that._btntype.picSel:\n						if (this._picSelbtn) break;\n						this._picSelbtn = new PicSel();\n						this._picSelbtn.initPicSel(this._margin, i);\n						break;\n					case that._btntype.website:\n						if (this._websitebtn) break;\n						this._websitebtn = new Website();\n						this._websitebtn.initWebsite(this._margin, i);\n						break;\n				}\n			}\n		};\n\n		ControlBar.prototype._getNumberOfBtn = function() {\n			var number = 0;\n			var tmp = 0;\n			for (var i = that._assets.ads.length - 1; i >= 0; i--) {\n				if (that._assets.ads[i].id > 1) {\n					tmp = tmp | that._assets.ads[i].id;\n				}\n			};\n			// number = tmp.toString(2).replace('0', '').length;\n			number = this._bitCount5(tmp);\n			return number;\n		};\n\n		//计算2进制数中1的个数\n		ControlBar.prototype._bitCount5 = function(n) {\n			var tmp = n - ((n >> 1) & 033333333333) - ((n >> 2) & 011111111111);\n			return ((tmp + (tmp >> 3)) & 030707070707) % 63;\n		}\n\n		ControlBar.prototype.changeStatus = function(selectIndex) {\n			if (selectIndex == this._selectedbtn) return;\n			if (this._selectedbtn > 0) {\n				switch (this._selectedbtn) {\n					case that._btntype.logo:\n						this._logobtn.resize(0);\n						break;\n					case that._btntype.share:\n						this._sharebtn.resize(0);\n						this._sharebtn.hideShareDiv();\n						break;\n					case that._btntype.tdCode:\n						this._tdCodebtn.resize(0);\n						this._tdCodebtn.hideTdCodeDiv();\n						break;\n					case that._btntype.videoSel:\n						this._videoSelbtn.resize(0);\n						this._videoSelbtn.hideVideoSelDiv();\n						break;\n					case that._btntype.picSel:\n						this._picSelbtn.resize(0);\n						this._picSelbtn.hidePicSelDiv();\n						break;\n					case that._btntype.website:\n						this._websitebtn.resize(0);\n						break;\n				}\n			}\n			this._selectedbtn = selectIndex;\n			switch (this._selectedbtn) {\n				case that._btntype.logo:\n					this._logobtn.resize(1);\n					break;\n				case that._btntype.share:\n					this._sharebtn.resize(1);\n					this._sharebtn.showShareDiv();\n					break;\n				case that._btntype.tdCode:\n					this._tdCodebtn.resize(1);\n					this._tdCodebtn.showTdCodeDiv();\n					break;\n				case that._btntype.videoSel:\n					this._videoSelbtn.resize(1);\n					this._videoSelbtn.showVideoSelDiv();\n					break;\n				case that._btntype.picSel:\n					this._picSelbtn.resize(1);\n					this._picSelbtn.showPicSelDiv();\n					break;\n				case that._btntype.website:\n					this._websitebtn.resize(1);\n					break;\n			}\n		};\n\n		ControlBar.prototype.pauseAd = function() {\n			switch (this._selectedbtn) {\n				case that._btntype.videoSel:\n					this._videoSelbtn.pause();\n				case that._btntype.picSel:\n					this._picSelbtn.pause();\n					break;\n			}\n		};\n		ControlBar.prototype.resumeAd = function() {\n			switch (this._selectedbtn) {\n				case that._btntype.videoSel:\n					this._videoSelbtn.start();\n				case that._btntype.PicSel:\n					this._picSelbtn.start();\n					break;\n			}\n		};\n		ControlBar.prototype._initialized = true;\n	};\n	var FuncBtn = function() {};\n\n	FuncBtn.prototype.initfuncBtn = function(marginsize) {\n		this._div = document.createElement('div');\n		this._div.style.cssText = 'width:50px;height:50px;display:-webkit-inline-box;margin-left:' + marginsize;\n		var innerhtml = '<img src=\"\" style=\"width:50px;height:50px\"/>';\n		this._div.innerHTML = innerhtml;\n		document.getElementById('controlbar').appendChild(this._div);\n	};\n\n	FuncBtn.prototype.display = function(selected, srcimg) {\n		if (!this._img) return;\n		if (selected == 1) {\n			this._img.style.cssText = 'width:60px;height:60px;margin-left:-5px;margin-top:-5px;';\n		} else {\n			this._img.style.cssText = 'width:50px;height:50px;';\n		}\n		this._img.src = srcimg[selected];\n	};\n\n	//分享按钮\n	var Share = function() {\n		this._div = null;\n		this._img = null;\n		this._sharediv = null;\n		this._index = null;\n		this._Creative = [];\n		this._number = 0;\n		this._sharebtn = [];\n		this._imgsrc = ['http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225175619_2-2.png', 'http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225175633_2-1.png'];\n		if (typeof Share.prototype._initialized != 'undefined') return;\n		Share.prototype.initShare = function(marginsize, index) {\n			this._index = index;\n			this.initfuncBtn(marginsize);\n			this._img = this._div.firstChild;\n			this.display(0, this._imgsrc);\n\n			this._getCreative();\n			this._initShareDiv();\n\n			this._addListener();\n		};\n		Share.prototype._getCreative = function() {\n			var index = 0;\n			var ads = null;\n			for (var i = 0, len = that._assets.ads.length; i < len; i++) {\n				if (that._assets.ads[i].id == that._btntype.share) {\n					this._Creative[index] = {\n						'adsindex': i,\n						'type': that._getExtensionValue(i, 'type'),\n						'jumpurl': this._generateJumpUrl(that._getExtensionValue(i, 'type'), that._getExtensionValue(i, 'url'), that._getExtensionValue(i, 'title'), that._getExtensionValue(i, 'pic')),\n						'innerhtml': this.generateInnerhtml(that._getExtensionValue(i, 'type'))\n					};\n					index++;\n					this._number++;\n				}\n			}\n		};\n		Share.prototype._generateJumpUrl = function(type, url, title, pic) {\n			var tencentRegx = /tencent/i;\n			var sinaRegx = /sina/i;\n			var jumpurl = null;\n			if (tencentRegx.test(type)) {\n				jumpurl = 'http://share.v.t.qq.com/index.php?c=share&a=index';\n			} else if (sinaRegx.test(type)) {\n				jumpurl = 'http://service.weibo.com/share/share.php?c=spr_web_bd_pplive_weibo';\n			}\n			url && (jumpurl += '&url=' + url);\n			title && (jumpurl += '&title=' + encodeURIComponent(title));\n			pic && (jumpurl += '&pic=' + pic);\n			return jumpurl;\n		};\n		Share.prototype.generateInnerhtml = function(type) {\n			var tencentRegx = /tencent/i;\n			var sinaRegx = /sina/i;\n			var innerhtml = null;\n			if (tencentRegx.test(type)) {\n				innerhtml = '<img src=\"http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225181638_tencent.png\" style=\"position:absolute;left:35px;top:30px;\"/><span style=\"color:white;left:110px;top:35px;font-size:35px;position:absolute;font-family:微软雅黑;\">腾讯微博</span><img src=\"http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225181652_sharebtn.png\" style=\"position:absolute;left:365px;top:44px;\">';\n			} else if (sinaRegx.test(type)) {\n				innerhtml = '<img src=\"http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225181738_sina.png\" style=\"position:absolute;left:35px;top:30px;\"/><span style=\"color:white;left:110px;top:35px;font-size:35px;position:absolute;font-family:微软雅黑;\">新浪微博</span><img src=\"http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225181652_sharebtn.png\" style=\"position:absolute;left:365px;top:44px;\">';\n			}\n			return innerhtml;\n		}\n		Share.prototype._initShareDiv = function() {\n			this._sharediv = document.createElement('div');\n			this._sharediv.style.cssText = 'display:none;position:absolute;left:265px;bottom:100px;width:498px;height:' + this._number * 111 + 'px;border:3px solid #333333;border-radius: 10px;z-index:2';\n			if (this._number == 1) {\n				var innerhtml = '<div id=\"div_0\" style=\"position:absolute;width:100%;height:100%\">' + this._Creative[0].innerhtml + '</div>';\n\n			} else {\n				var innerhtml = '<div id=\"div_0\" style=\"position:absolute;width:498px;height:111px;border-bottom:3px solid #333333\">' + this._Creative[0].innerhtml + '</div><div id=\"div_1\" style=\"position:absolute;width:498px;height:111px;top:111px;\">' + this._Creative[1].innerhtml + '</div>';\n			}\n			this._sharediv.innerHTML = innerhtml;\n			document.getElementById('bgdiv').appendChild(this._sharediv);\n\n			for (var i = 0; i < this._number; i++) {\n				this._sharebtn[i] = document.getElementById('div_' + i).childNodes[2];\n			}\n			// that.resizeAd.call(that,245,105,null,49/21,this._sharediv);\n		};\n\n		Share.prototype.resize = function(selected) {\n			this.display(selected, this._imgsrc);\n		};\n\n		Share.prototype._addListener = function() {\n			var self = this;\n			this._img.addEventListener('click', function() {\n				that.onAd_Controlbar_click(that._btntype.share);\n				that._executeSubscribe('ad_controlbar_click', [self._index]);\n			});\n\n			for (var i = 0; i < this._number; i++) {\n				this._addSharebtnListener(i);\n			}\n		};\n		Share.prototype._addSharebtnListener = function(index) {\n			var self = this;\n			this._sharebtn[index].addEventListener('click', function() {\n				that._executeSubscribe('ad_video_click', [self._Creative[index].jumpurl, self._Creative[index].adsindex, true]);\n			})\n		};\n		Share.prototype.showShareDiv = function() {\n			this._sharediv.style.display = '';\n		};\n		Share.prototype.hideShareDiv = function() {\n			this._sharediv.style.display = 'none';\n		};\n		Share.prototype._initialized = true;\n	};\n	Extend(FuncBtn, Share);\n\n	//二维码按钮\n	var TdCode = function() {\n		this._div = null;\n		this._img = null;\n		this._pic = null;\n		this._tdCodeDiv = null;\n		this._index = null;\n		this._imgsrc = ['http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225175716_3-2.png', 'http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225175705_3-1.png'];\n		if (typeof TdCode.prototype._initialized != 'undefined') return;\n		TdCode.prototype.initTdCode = function(marginsize, index) {\n			this._index = index;\n			this.initfuncBtn(marginsize);\n			this._img = this._div.firstChild;\n			this.display(0, this._imgsrc);\n			this.initTdCodeDiv();\n			this._addListener();\n		};\n		TdCode.prototype.initTdCodeDiv = function() {\n			this._tdCodeDiv = document.createElement('div');\n			this._tdCodeDiv.style.cssText = 'display:none;position:absolute;left:420px;top:290px;width:180px;height:180px;z-index:2';\n			var innerhtml = '<img src=\"' + that._assets.ads[this._index].vd + '\" style=\"height:100%;width:100%\"/>';\n			this._tdCodeDiv.innerHTML = innerhtml;\n			this._pic = this._tdCodeDiv.firstChild;\n			document.getElementById('bgdiv').appendChild(this._tdCodeDiv);\n		}\n		TdCode.prototype.resize = function(selected) {\n			this.display(selected, this._imgsrc);\n		};\n		TdCode.prototype._addListener = function() {\n			var self = this;\n			this._img.addEventListener('click', function() {\n				that.onAd_Controlbar_click(that._btntype.tdCode);\n				that._executeSubscribe('ad_controlbar_click', [self._index]);\n			});\n			this._pic.addEventListener('click', function() {\n				that._executeSubscribe('ad_video_click', [that._assets.ads[self._index].vdjump, self._index, true]);\n			});\n		};\n		TdCode.prototype.showTdCodeDiv = function() {\n			this._tdCodeDiv.style.display = '';\n		};\n		TdCode.prototype.hideTdCodeDiv = function() {\n			this._tdCodeDiv.style.display = 'none';\n		};\n		TdCode.prototype._initialized = true;\n	};\n	Extend(FuncBtn, TdCode);\n\n	//下方中间的Logo\n	var Logo = function() {\n		this._div = null;\n		this._index = null;\n		if (typeof Logo.prototype._initialized != 'undefined') return;\n		Logo.prototype.initLogo = function(marginsize, index) {\n			this._index = index;\n			this._div = document.createElement('div');\n			this._div.style.cssText = 'width:90px;height:90px;display:-webkit-inline-box;margin-top:-30px;margin-left:' + marginsize;\n			var innerhtml = '<img src=\"' + that._assets.ads[this._index].vd + '\" style=\"width:100%;height:100%\"/>';\n			this._div.innerHTML = innerhtml;\n			document.getElementById('controlbar').appendChild(this._div);\n		};\n		Logo.prototype._initialized = true;\n	}\n\n	//互动栏上的多贴片按钮\n	var VideoSel = function() {\n		this._div = null;\n		this._img = null;\n		this._index = null;\n		this._imgsrc = ['http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225175936_4-2.png', 'http://asimgs.pplive.cn/imgs/materials/2013/12/25/20131225175910_4-1.png'];\n		this._Creative = [];\n		this._videoView = null;\n		if (typeof VideoSel.prototype._initialized != 'undefined') return;\n		VideoSel.prototype.initVideoSel = function(marginsize, index) {\n			this._index = index;\n			this.initfuncBtn(marginsize);\n			this._img = this._div.firstChild;\n			this.display(0, this._imgsrc);\n			this._addListener();\n			this._init();\n		};\n		VideoSel.prototype._init = function() {\n			this._getCreative();\n			this._videoView = new VideoView();\n			this._videoView.initVideoView(this._Creative);\n		};\n\n		VideoSel.prototype._getCreative = function() {\n			var index = 0;\n			for (var i = 0, len = that._assets.ads.length; i < len; i++) {\n				if (that._assets.ads[i].id == that._btntype.videoSel) {\n					this._Creative[index] = {\n						'adsindex': i,\n						'largevideo': that._assets.ads[i].vd,\n						'vdjump': that._assets.ads[i].vdjump,\n						'smallpic': that._assets.ads[i].poster\n					};\n					index++;\n				}\n			};\n		};\n		VideoSel.prototype.pause = function() {\n			this._videoView.pause();\n		};\n		VideoSel.prototype.start = function() {\n			this._videoView.start();\n		};\n		VideoSel.prototype.showVideoSelDiv = function() {\n			this._videoView.show();\n		};\n		VideoSel.prototype.hideVideoSelDiv = function() {\n			this._videoView.hide();\n		};\n		VideoSel.prototype.resize = function(selected) {\n			this.display(selected, this._imgsrc);\n		};\n		VideoSel.prototype._addListener = function() {\n			var self = this;\n			this._img.addEventListener('click', function() {\n				that.onAd_Controlbar_click(that._btntype.videoSel);\n				that._executeSubscribe('ad_controlbar_click', [self._index]);\n			});\n		}\n		VideoSel.prototype._initialized = true;\n	};\n	Extend(FuncBtn, VideoSel);\n\n	//选择贴片的展示区域\n	var VideoView = function() {\n		this._Creative = null;\n		this._index = 0;\n		this._ViewDiv = null;\n		this._largeVideo = null;\n		this._choose = null;\n\n		if (typeof VideoView.prototype._initialized != 'undefined') return;\n\n		var LargeVideo = function() {\n			this._largeVideoDiv = null;\n			this._video = null;\n			this._lastvideo = null;